<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <style type="text/css">
        body {
            font-family: 'Calibri';
            font-size: 12pt;
            margin: 0 30px;
        }

        div {
            width: 100%;
            clear: both;
            margin: 10px 0;
        }

        table {
            margin-top: 0pt;
            margin-bottom: 0pt;
            border-collapse: collapse;
            width: 100%
        }

        td {
            border-style: solid;
            border-width: 0.75pt;
            padding: 5px;
        }</style>
</head>
<body>
    @RenderBody()
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
